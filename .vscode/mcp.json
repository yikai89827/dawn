{
    "servers": {
        "shopify-dev-mcp":{
            "command": "npx",
            "args": [
                "-y",
                "@shopify/dev-mcp@latest"
            ],
            "env": {
                "LIQUID": "true", //支持liquid语法
                "LIQUID_VALIDATION_MODE": "partial" //自动代码审查,使用和文档实时的语法
            }
        }
    }
}